# .gitignore for Python projects
# Ignore Python compiled files
__pycache__/
*.py[cod]
*$py.class

# C extensions
*.so

# Distribution / packaging
*.egg-info/
*.egg
dist/
build/
.eggs/

# Virtual environments
# Common names: venv, env, .venv, .env, SOA_venv
venv/
env/
.venv/
.SOA_venv/
SOA_venv/
ENV/

# Installer logs
pip-log.txt
pip-delete-this-directory.txt

# Unit test / coverage
.pytest_cache/
.coverage
.coverage.*
coverage.xml
htmlcov/

# Pyre type checker
.pyre/

# PyCharm
.idea/

# VS Code
.vscode/

# macOS
.DS_Store

# Logs and runtime files
*.log
instance/
*.sqlite3
*.db
*.sqlite

# Byte-compiled / optimized / DLL files
*.pyc
*.pyo
*.pyd

# mypy
.mypy_cache/

# wheel
pip-wheel-metadata/

# temporary files
*.swp
*~

# Generated by Python setup tools
.eggs/
*.egg-info/

# local env files
.env

# Keep these safe
!.env.example